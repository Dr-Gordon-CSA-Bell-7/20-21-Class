/* package whatever; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Ideone
{
	public static void main (String[] args) throws java.lang.Exception
	{
		for(int x=-10; x<=10; x++){
			System.out.print(x);
		}
		int acc = -10;
		System.out.println();
		for(int x =-10; x<= 10; x++){
			System.out.print(acc);
			acc++;
		}
	}
}
